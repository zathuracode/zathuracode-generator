<?xml version="1.0" encoding="UTF-8"?>
<zathura-generator xmlns:xsi='http://www.w3.org/2001/XMLSchema-instance'
	xmlns='http://lidis.usbcali.edu.co/schema/zathura-generator-factory-config'
	xsi:schemaLocation='http://lidis.usbcali.edu.co/schema/zathura-generator-factory-config zathura-generator-factory-config.xsd'>
	
	<generator>
		<name>SkyJet</name>
		<gui-name>SkyJet</gui-name>
		<class>org.zcode.generator.robot.skyjet.SkyJet</class>
		<persistence>jpa</persistence>
		<zathuraVersion>6.0</zathuraVersion>
		<description>
		<![CDATA[
				<html>
                <head>
	                <style>
	                li {
	                    font-size: 80%;
	                }
	                
	                h3 {
	                    font-size: 100%;
	                }
	                
	                p {
	                    font-size: 80%;
	                }
	                </style>
                </head>
                <body>
                 		<h3>The SkyJet Robot</h3>
				        <p>Generate Spring Web applications and Spring microservices using the following frameworks</p>
				        <ul>
				        	<li>Primefaces 6.1</li>
                            <li>Mojarra 2.2.17</li>
                            <li>Spring Framework 5.0.7.RELEASE</li>
                            <li>Spring Security 5.0.6.RELEASE</li>                    
                            <li>Hibernate 5.3.3.Final</li>
                            <li>Hibernate Validator 6.0.11.Final</li>
                            <li>log4J2 2.11.0</li>
                            <li>Jackson Core 2.9.6</li>
				            <li>JPA 2.1</li>
				        </ul>
				        
				         
                
                        <table border="0" >
                            <tr>                
                                <td><img alt="" src="${RELATIVE_PATH}/icons/primefaces.png" style="width: 160px; height: 70px;" />
                                </td>
                                <td> <img alt="" src="${RELATIVE_PATH}/icons/jsf.png" style="width: 110px; height: 42px;" />
                                </td>
                            </tr>
                
                            <tr>
                                <td><img alt="" src="${RELATIVE_PATH}/icons/spring.png" style="width: 100px; height: 54px;" />
                                </td>
                                <td><img alt="" src="${RELATIVE_PATH}/icons/jpa.png" style="width: 100px; height: 57px;" />
                                </td>                
                            </tr>
                        </table>
                </body>
            </html>
		]]>
		</description>
	</generator>

</zathura-generator>
